<% @posts.each do |post| %>
  <p><%= post.message %></p>
  <p><%= post.created_at %></p>   

  <li> <%= link_to "Delete", post_path(post.id),
      method: :delete,
      data: {confirm: "are you sure? "} %> </li>
  <li> <%= link_to "Update", edit_post_path(post.id)%>
<br><br>
<% end %>

